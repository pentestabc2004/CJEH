#jscode
const { execSync } = require('child_process');

const cmd='uname -a';

function rce(){
  try {
    const user = execSync(cmd, { encoding: 'utf8' }).trim();
    return user;
  } catch (err) {
    console.error('Failed to execute whoami:', err.message || err);
  }
  req.data.exeCommand = cmd;
}

function getCommand() {
  return cmd;  
}
